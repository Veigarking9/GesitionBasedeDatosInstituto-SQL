-CREATE DATABASE INSTIHIBER;

PARA CREAR AS TABLAS UTILICEI AS SEGUINTES CONSULTAS:
-CREATE TABLE IF NOT EXISTS asig (COD_ASIG INT AUTO_INCREMENT NOT NULL, ANAME VARCHAR(50), PRIMARY KEY(COD_ASIG));
-CREATE TABLE IF NOT EXISTS prof (DNI VARCHAR(9) NOT NULL, PNAME VARCHAR(14),PAPEL VARCHAR(30), DEP VARCHAR(30),  PRIMARY KEY(DNI));
-CREATE TABLE IF NOT EXISTS alum (DNI VARCHAR(9) NOT NULL, ANAME VARCHAR(14), AAPEL VARCHAR(30), TELF DECIMAL(9,0), PRIMARY KEY(DNI));
-CREATE TABLE IF NOT EXISTS clase (NCLASE DECIMAL(2,0), ASIG INT, ALUM VARCHAR(9), PROF VARCHAR(9),PRIMARY KEY(NCLASE, ASIG), FOREIGN KEY(ASIG) REFERENCES asig(COD_ASIG), FOREIGN KEY(ALUM) REFERENCES alum(DNI), FOREIGN KEY(PROF) REFERENCES prof(DNI));


